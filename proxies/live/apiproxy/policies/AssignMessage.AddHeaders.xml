<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.AddHeaders">
  <Set>
    <Headers>
      <Header name="FromASID">{verifyapikey.VerifyAPIKey.CustomAttributes.asid}</Header>
      <Header name="ToASID">{private.apigee.ToASID}</Header>
      <Header name="TraceID">{request.header.X-Request-ID}</Header>
    </Headers>
  </Set>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>
